T_NAME		plw26
		TYPE	R32
		SUBRANGE 0 639.11
		CLASS	ACQU
		REL	"plw26=plw3*pow( (p5/90)/(p62/90), 2)"
		INV_REL	"D[0]=plw26"
		UNIT	"dB"
		TEXT	"plw26=plw3*pow( (p5/90)/(p62/90), 2)"
		FORMAT	"%14.5g W"
