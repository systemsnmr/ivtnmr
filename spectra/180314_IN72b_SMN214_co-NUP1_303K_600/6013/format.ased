EDIT_PAR COMMAND FILE

DI_MODE         LONG
ED_ENTRY        ACQU
ORDER           FILE


HEADER		"====== General ======"
NAME		PULPROG
END
NAME		TD
END
NAME		SW
END
T_NAME		SWH
		TYPE	R32
		CLASS	ACQU
		SUBRANGE 0.0 1e38
		REL	"SWH=SW*SFO1"
		INV_REL "SW=SWH/SFO1"
		UNIT	"Hz"
		FORMAT	"%14.2f Hz"
		TEXT	"sweep width"
END
T_NAME          AQ
		TYPE	R32
		CLASS	ACQU
		SUBRANGE 0.0 1e38
		REL	"AQ=aqcalc(SW,SFO1,TD,DIGTYP)"
		INV_REL	"TD=tdcalc(SW,SFO1,1.0e6*AQ,DIGTYP)"
		UNIT	"sec"
		FORMAT	"%14.7f sec"
                TEXT	"acquisition time"
END
NAME		RG
END
T_NAME          DW
		TYPE	R32
		CLASS	ACQU
		SUBRANGE 0.0 1e38
		REL	"DW=1/(2*SW*SFO1)"
		INV_REL	"SW=1000000/(2*(0.005+DW)*SFO1)"
		UNIT	"usec"
		FORMAT	"%14.3f usec"
		TEXT	"dwell time "
END
NAME		DE
		FORMAT	"%14.2f usec"
END
T_NAME		CNST19
		TYPE	R32
		CLASS	ACQU
		SUBRANGE -1e38 1e38
		REL	"CNST19=CNST[19]"
		INV_REL	"CNST[19]=CNST19"
		FORMAT	"%14.7f"
		TEXT		"chem.shift for protons of interest (offset, in ppm)"
END
T_NAME		cnst39
		TYPE	R32
		SUBRANGE -1e38 1e38
		CLASS	ACQU
		REL	"cnst39=0.529"
		INV_REL	"D[0]=cnst39"
		UNIT	"no"
		TEXT		"To calc compensation of chem.shift evol. during p39 (for Pc9_4_120.1000: 0.529)"
		FORMAT	"%14.6f"
		NONEDIT
END
T_NAME		CNST63
		TYPE	R32
		CLASS	ACQU
		SUBRANGE -1e38 1e38
		REL	"CNST63=CNST[63]"
		INV_REL	"CNST[63]=CNST63"
		FORMAT	"%14.7f"
		TEXT		"spectral width of signals of interest (in ppm)"
END
T_NAME		D1
		TYPE	R32
		CLASS	ACQU
		SUBRANGE 0.0 1e38
		REL	"D1=D[1]"
		INV_REL	"D[1]=D1"
		UNIT	"sec"
		FORMAT	"%14.8f sec"
		TEXT		"relaxation delay (~0.1s in zg-sofast, ~1s in zg-wg)"
END
T_NAME		d39
		TYPE	R32
		SUBRANGE 0.0 1e8
		CLASS	ACQU
		REL	"d39=p39*cnst39"
		INV_REL	"D[0]=d39"
		UNIT	"sec"
		TEXT		"Compensates chem.shift evol. during p39 (p39*cnst39)"
		FORMAT	"%14.8f sec"
		NONEDIT
END
NAME		DS
		TEXT	"number of dummy scans"
END
NAME		NS
		TEXT	"scans to execute"
END
T_NAME		P1
		TYPE	R32
		CLASS	ACQU
		SUBRANGE 0.0 1.0e10
		REL	"P1=P[1]"
		INV_REL	"P[1]=P1"
		UNIT	"usec"
		FORMAT	"%14.2f usec"
		TEXT		"90 degree hard pulse 1H"
END
T_NAME		p2
		TYPE	R32
		SUBRANGE -1e38 1e38
		CLASS	ACQU
		REL	"p2=2*p1"
		INV_REL	"D[0]=p2"
		UNIT	"usec"
		TEXT	"p2=2*p1"
		FORMAT	"%14.2f usec"
		NONEDIT
END
NAME		ZGOPTNS
		TEXT 	"options for zg"
END
HEADER		"======== CHANNEL f1 ========"
NAME		SFO1
		FORMAT	"%14.7f MHz"
		TEXT	"frequency of ch. 1"
END
NAME		O1
		FORMAT	"%14.2f Hz"
		TEXT	"frequency of ch. 1"
END
T_NAME		O1P
		TYPE	R64
		CLASS	ACQU
		SUBRANGE -1e38 1e38
		REL	"O1P=O1/BF1"
		INV_REL	"O1=O1P*BF1"
		UNIT	"ppm"
		FORMAT	"%14.3f ppm"
		TEXT	"frequency of ch. 1"
END
NAME		NUC1
		NONEDIT
		TEXT	"nucleus for channel 1"
END
T_NAME		p39
		TYPE	R32
		SUBRANGE -1e38 1e38
		CLASS	ACQU
		REL	"p39=(7.512/(bf1*cnst63))*1000000"
		INV_REL	"D[0]=p39"
		UNIT	"usec"
		TEXT		"120 deg shaped pulse for excitation (~3ms at 600MHz)"
		FORMAT	"%14.2f usec"
		NONEDIT
END
T_NAME		p40
		TYPE	R32
		SUBRANGE -1e38 1e38
		CLASS	ACQU
		REL	"p40=(2.332/(bf1*cnst63))*1000000"
		INV_REL	"D[0]=p40"
		UNIT	"usec"
		TEXT		"180 deg shaped pulse for refocusing (~1ms at 600MHz)"
		FORMAT	"%14.2f usec"
		NONEDIT
END
T_NAME		PLW1
		TYPE	R32
		CLASS	ACQU
		SUBRANGE 0 52.7973
		REL	"PLW1=PLW[1]"
		INV_REL	"PLW[1]=PLW1"
		UNIT	"W"
		FORMAT	"%14.5g W"
		TEXT		"power level for hard 1H pulse"
END
T_NAME		PLdB1
		TYPE	R32
		CLASS	ACQU
		SUBRANGE -17.3261 1000
		REL	"if(PLW[1]>0)PLdB1=-10*log(PLW[1])/LN10;else PLdB1=1000;"
		INV_REL	"PLW[1]=0;if(PLdB1<1000)PLW[1]=pow(10.0,(-PLdB1)/10.0)"
		UNIT	"dB"
		FORMAT	"%14.2f -dBW"
		TEXT	"power PL1 in -dBW"
END
NAME		SPNAM[39]
		TEXT		"Pc9_4_120.1000"
END
T_NAME		SPOAL39
		TYPE	R32
		CLASS	ACQU
		SUBRANGE -1.0 1.0
		REL	"SPOAL39=SPOAL[39]"
		INV_REL	"SPOAL[39]=SPOAL39"
		FORMAT	"%14.3f"
		TEXT	"phase alignment of freq. offset in SP39"
END
T_NAME		spoffs39
		TYPE	R32
		SUBRANGE -1e38 1e38
		CLASS	ACQU
		REL	"spoffs39=bf1*(cnst19/1000000)-o1"
		INV_REL	"D[0]=spoffs39"
		UNIT	"Hz"
		TEXT	"spoffs39=bf1*(cnst19/1000000)-o1"
		FORMAT	"%14.2f Hz"
		NONEDIT
END
T_NAME		spw39
		TYPE	R32
		SUBRANGE 0 52.7973
		CLASS	ACQU
		REL	"spw39=plw1*pow((p1/90)/((p39/120)*0.125),2)"
		INV_REL	"D[0]=spw39"
		UNIT	"dB"
		TEXT	"spw39=plw1*pow((p1/90)/((p39/120)*0.125),2)"
		FORMAT	"%14.5g W"
		NONEDIT
END
T_NAME		SPdB39
		TYPE	R32
		CLASS	ACQU
		NONEDIT
		SUBRANGE -17.3261 1000
		REL	"if(spw39>0)SPdB39=-10*log(spw39)/LN10;elseSPdB39=1000;"
		INV_REL	"SPW[39]=0;if(SPdB39<1000)SPW[39]=pow(10.0,(-SPdB39)/10.0)"
		UNIT	"dB"
		FORMAT	"%14.2f -dBW"
		TEXT	"power SPW39 in -dBW"
END
NAME		SPNAM[40]
		TEXT		"Rsnob.1000"
END
T_NAME		SPOAL40
		TYPE	R32
		CLASS	ACQU
		SUBRANGE -1.0 1.0
		REL	"SPOAL40=SPOAL[40]"
		INV_REL	"SPOAL[40]=SPOAL40"
		FORMAT	"%14.3f"
		TEXT	"phase alignment of freq. offset in SP40"
END
T_NAME		spoffs40
		TYPE	R32
		SUBRANGE -1e38 1e38
		CLASS	ACQU
		REL	"spoffs40=bf1*(cnst19/1000000)-o1"
		INV_REL	"D[0]=spoffs40"
		UNIT	"Hz"
		TEXT	"spoffs40=bf1*(cnst19/1000000)-o1"
		FORMAT	"%14.2f Hz"
		NONEDIT
END
T_NAME		spw40
		TYPE	R32
		SUBRANGE 0 52.7973
		CLASS	ACQU
		REL	"spw40=plw1*pow((p1/90)/((p40/180)*0.2137),2)"
		INV_REL	"D[0]=spw40"
		UNIT	"dB"
		TEXT	"spw40=plw1*pow((p1/90)/((p40/180)*0.2137),2)"
		FORMAT	"%14.5g W"
		NONEDIT
END
T_NAME		SPdB40
		TYPE	R32
		CLASS	ACQU
		NONEDIT
		SUBRANGE -17.3261 1000
		REL	"if(spw40>0)SPdB40=-10*log(spw40)/LN10;elseSPdB40=1000;"
		INV_REL	"SPW[40]=0;if(SPdB40<1000)SPW[40]=pow(10.0,(-SPdB40)/10.0)"
		UNIT	"dB"
		FORMAT	"%14.2f -dBW"
		TEXT	"power SPW40 in -dBW"
END
