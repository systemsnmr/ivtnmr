		TYPE	R32
		CLASS	ACQU
		SUBRANGE 0 639.11
		REL	"PLW3=PLW[3]"
		INV_REL	"PLW[3]=PLW3"
		UNIT	"W"
		FORMAT	"%14.5g W"
